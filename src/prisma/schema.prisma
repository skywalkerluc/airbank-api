// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id              String     @id
  account         String
  description     String?
  category        String?
  reference       String?
  currency        String
  amount          Float
  status          String
  transactionDate String
  createdAt       String
  updatedAt       String
}
